{
  "lock": false,
  "unstable": ["kv"],
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "start": "deno run -A --env --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A --env main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "server": "deno run -A --env server.ts"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "_fresh"
  ],
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "@preact/signals": "npm:@preact/signals@^1.3.0",
    "@preact/signals-core": "npm:@preact/signals-core@^1.8.0",
    "@sholvoir/components/": "https://raw.esm.sh/gh/sholvoir/preact-components@0.0.8/",
    "@sholvoir/generic": "jsr:@sholvoir/generic@^0.0.10",
    "@std/http": "jsr:@std/http@^1.0.8",
    "js-cookie": "https://esm.sh/js-cookie@3.0.5",
    "preact": "npm:preact@^10.24.3",
    "preact/jsx-runtime": "npm:preact@^10.24.3/jsx-runtime",
    "preact/hooks": "npm:preact@^10.24.3/hooks",
    "tailwindcss": "npm:tailwindcss@^3.4.14",
    "tailwindcss/": "npm:/tailwindcss@^3.4.14/",
    "tailwindcss/plugin": "npm:/tailwindcss@^3.4.14/plugin.js"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
